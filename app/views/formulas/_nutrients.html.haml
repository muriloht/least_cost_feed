%tr.fields.nutrient{style: hide_if_destroyed(f.object)}
  = f.hidden_field :nutrient_id, class: 'nutrient_id'
  = f.hidden_field :_destroy, class: 'destroy'
  %td.check-text-box.col-xs-5
    = f.check_box :use
    = f.input_field :nutrient_name_unit, readOnly: true, tabIndex: -1
  %td= f.input_field :min, type: :text, class: :numeric
  %td= f.input_field :max, type: :text, class: :numeric
  %td= f.input_field :actual, readOnly: true, type: :text, class: :numeric, tabIndex: -1
